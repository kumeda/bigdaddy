    <div class="col-lg-12">
      <h1 class="page-header">Show User</h1>
    </div>
  </div>
  <!-- /.row -->

<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

